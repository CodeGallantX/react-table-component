{
  "name": "lightweight-react-table-component",
  "description": "A simple table component in react inspired by bootstrapVue table component, using bootstrap css",
  "private": false,
  "version": "1.0.1",
  "keywords": [
    "react",
    "components",
    "ui",
    "table",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamezzz123/react-table-component"
  },
  "type": "module",
  "author": {
    "name": "James Chinwokwu",
    "email": "chinwokwuj@gmail.com"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-table-component.umd.js",
  "module": "./dist/react-table-component.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-table-component.es.js",
      "require": "./dist/react-table-component.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0"
  }
}